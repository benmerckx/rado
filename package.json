{
  "name": "rado",
  "version": "0.4.9",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && tsx build.ts",
    "size": "esbuild src/index.ts --bundle --minify --format=esm --outdir=dist",
    "profile": "cross-env PROFILE=true tsx build.ts && cd bin && (rimraf CPU*.cpuprofile || true) && node --cpu-prof --cpu-prof-interval=100 test && speedscope CPU*.cpuprofile",
    "prepublishOnly": "tsc -p tsconfig.build.json && tsx build.ts",
    "cycles": "madge --warning --circular src/index.ts",
    "test:bun": "bun run test/index.ts",
    "test:node": "tsx test/index.ts",
    "test:deno": "deno run --allow-read --allow-env test/index.ts"
  },
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": "./dist/index.js",
    "./*": "./dist/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@biomejs/biome": "^1.6.1",
    "@cloudflare/workers-types": "^4.20230628.0",
    "@electric-sql/pglite": "^0.1.3",
    "@sinclair/carbon": "^0.8.3",
    "@sqlite.org/sqlite-wasm": "^3.42.0-build4",
    "@types/better-sqlite3": "^5.4.1",
    "@types/glob": "^8.0.0",
    "@types/sql.js": "^1.4.2",
    "better-sqlite3": "^9.5.0",
    "bun-types": "^0.5.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.18.11",
    "glob": "^8.0.3",
    "madge": "^6.1.0",
    "rimraf": "^4.1.2",
    "sade": "^1.8.1",
    "speedscope": "^1.15.0",
    "sql.js": "^1.8.0",
    "sqlite3": "^5.1.6",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5",
    "uvu": "^0.5.6"
  },
  "volta": {
    "node": "20.12.1"
  },
  "packageManager": "yarn@4.1.1",
  "dependencies": {
    "prettier": "^3.2.5"
  }
}
