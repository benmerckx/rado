{
  "name": "rado",
  "version": "0.2.20",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js",
    "./driver/bun:sqlite": "./dist/driver/bun-sqlite.js"
  },
  "typesVersions": {
    "*": {
      "driver/bun:sqlite": [
        "./dist/driver/bun-sqlite.d.ts"
      ],
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.build.json && tsx build.ts",
    "test": "tsx test",
    "test:bun": "bun test/index.ts --driver=bun:sqlite",
    "profile": "cross-env PROFILE=true tsx build.ts && cd bin && (rimraf CPU*.cpuprofile || true) && node --cpu-prof --cpu-prof-interval=100 test && speedscope CPU*.cpuprofile",
    "prepublishOnly": "tsc -p tsconfig.build.json && tsx build.ts"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^5.4.1",
    "@types/glob": "^8.0.0",
    "@types/sql.js": "^1.4.2",
    "better-sqlite3": "^7.5.1",
    "bun-types": "^0.5.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.17.7",
    "glob": "^8.0.3",
    "rimraf": "^4.1.2",
    "sade": "^1.8.1",
    "speedscope": "^1.15.0",
    "sql.js": "^1.8.0",
    "sqlite3": "^5.1.4",
    "tsx": "^3.12.3",
    "typescript": "next",
    "uvu": "^0.5.6"
  },
  "volta": {
    "node": "18.12.1"
  },
  "packageManager": "yarn@3.3.1",
  "resolutions": {
    "esbuild": "0.17.7"
  }
}
