{
  "name": "rado",
  "version": "0.4.9",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js",
    "./driver/bun:sqlite": "./dist/driver/bun-sqlite.js"
  },
  "typesVersions": {
    "*": {
      "driver/*": [
        "dist/driver/*.d.ts"
      ],
      "lib/*": [
        "dist/lib/*.d.ts"
      ],
      "driver/bun:sqlite": [
        "dist/driver/bun-sqlite.d.ts"
      ],
      "define/*": [
        "dist/define/*.d.ts"
      ],
      "sqlite/*": [
        "dist/sqlite/*.d.ts"
      ],
      "sqlite": [
        "dist/sqlite.d.ts"
      ],
      "util/*": [
        "dist/util/*.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.build.json && tsx build.ts",
    "size": "esbuild v1/index.ts --bundle --minify --outdir=dist",
    "test": "tsx test",
    "test:bun": "bun test/index.ts --driver=bun:sqlite",
    "profile": "cross-env PROFILE=true tsx build.ts && cd bin && (rimraf CPU*.cpuprofile || true) && node --cpu-prof --cpu-prof-interval=100 test && speedscope CPU*.cpuprofile",
    "prepublishOnly": "tsc -p tsconfig.build.json && tsx build.ts",
    "cycles": "madge --warning --circular v1/index.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.6.1",
    "@cloudflare/workers-types": "^4.20230628.0",
    "@electric-sql/pglite": "^0.1.1",
    "@sqlite.org/sqlite-wasm": "^3.42.0-build4",
    "@types/better-sqlite3": "^5.4.1",
    "@types/glob": "^8.0.0",
    "@types/sql.js": "^1.4.2",
    "better-sqlite3": "^7.5.1",
    "bun-types": "^0.5.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.18.11",
    "glob": "^8.0.3",
    "madge": "^6.1.0",
    "rimraf": "^4.1.2",
    "sade": "^1.8.1",
    "speedscope": "^1.15.0",
    "sql.js": "^1.8.0",
    "sqlite3": "^5.1.6",
    "tsx": "^3.12.3",
    "typescript": "beta",
    "uvu": "^0.5.6"
  },
  "volta": {
    "node": "20.12.1"
  },
  "packageManager": "yarn@3.3.1"
}
